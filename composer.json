{
    "name": "tienvx/tchess",
    "license": "MIT",
    "repositories": [{
        "type": "package",
        "package": {
            "version": "dev-master",
            "name": "tienvx/chessboardjs",
            "type": "js-vendor",
            "source": {
                "url": "https://github.com/oakmac/chessboardjs.git",
                "type": "git",
                "reference": "master"
            }
        }
    },
    {
        "type": "package",
        "package": {
            "version": "v0.3.0",
            "name": "tienvx/chessboardjs",
            "type": "js-vendor",
            "dist": {
                "url": "http://chessboardjs.com/releases/0.3.0/chessboardjs-0.3.0.zip",
                "type": "zip"
            }
        }
    },
    {
        "type": "vcs",
        "url": "git@github.com:tienvx/js-vendor-installer-plugin.git"
    },
    {
        "type": "package",
        "package": {
            "name": "tienvx/jquery",
            "version": "v1.11.2",
            "type": "js-vendor",
            "dist": {
                "url": "http://code.jquery.com/jquery-1.11.2.min.js",
                "type": "file"
            }
        }
    },
    {
        "type": "package",
        "package": {
            "name": "tienvx/bootstrap",
            "version": "v3.3.2",
            "type": "js-vendor",
            "dist": {
                "url": "https://github.com/twbs/bootstrap/releases/download/v3.3.2/bootstrap-3.3.2-dist.zip",
                "type": "zip"
            }
        }
    },
    {
        "type": "package",
        "package": {
            "name": "tienvx/chessjs",
            "version": "dev-master",
            "type": "js-vendor",
            "dist": {
                "url": "https://github.com/jhlywa/chess.js/archive/master.zip",
                "type": "zip"
            }
        }
    },
    {
        "type": "package",
        "package": {
            "version": "v0.9.3",
            "name": "tienvx/autobahnjs",
            "type": "js-vendor",
            "dist": {
                "url": "http://autobahn.s3.amazonaws.com/js/autobahn.min.js",
                "type": "file"
            }
        }
    }],
    "require": {
        "php": ">=5.4.0",
        "symfony/class-loader": "v2.6.4",
        "symfony/http-foundation": "v2.6.4",
        "symfony/routing": "v2.6.4",
        "symfony/http-kernel": "v2.6.4",
        "symfony/event-dispatcher": "v2.6.4",
        "symfony/dependency-injection": "v2.6.4",
        "symfony/config": "v2.6.4",
        "symfony/yaml": "v2.6.4",
        "doctrine/orm": "v2.4.7",
        "symfony/serializer": "v2.6.4",
        "twig/twig": "v1.18.0",
        "symfony/twig-bridge": "v2.6.4",
        "symfony/form": "v2.6.4",
        "kriswallsmith/assetic": "v1.2.1",
        "symfony/validator": "v2.6.4",
        "tienvx/js-vendor-installer-plugin": "v1.1.1",
        "tienvx/chessboardjs": "v0.3.0",
        "tienvx/jquery": "v1.11.2",
        "tienvx/bootstrap": "v3.3.2",
        "packagist/yuicompressor-bin": "v2.4.8",
        "monolog/monolog": "1.12.0",
        "cboden/ratchet": "v0.3.2",
        "react/zmq": "v0.3.0",
        "tienvx/autobahnjs": "v0.9.3",
        "tienvx/chessjs": "dev-master",
        "symfony/browser-kit": "v2.6.4",
        "symfony/css-selector": "v2.6.4"
    },
    "autoload": {
        "psr-4": {
            "Tchess\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { "Tchess\\Tests\\": "tests/" }
    },
    "extra": {
        "installer-paths": {
            "web/js-vendor/tienvx/chessboardjs/": ["tienvx/chessboardjs"],
            "web/js-vendor/tienvx/autobahnjs/": ["tienvx/autobahnjs"],
            "web/js-vendor/tienvx/jquery/": ["tienvx/jquery"],
            "web/js-vendor/tienvx/bootstrap/": ["tienvx/bootstrap"],
            "web/js-vendor/tienvx/chessjs/": ["tienvx/chessjs"]
        }
    }
}
